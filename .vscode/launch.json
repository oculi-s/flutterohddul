    "version": "0.2.0",
    "configurations": [
        {
            "name": "오떨",
            "request": "launch",
            "type": "dart",
            "args": [
                "--web-port",
                "3000",
            ],
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/lib/main.dart",
            "webEnableHttps": true
        },
        {
            "name": "flutterohddul (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "flutterohddul (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }
    ]
}